{
  "name": "firebase-basic",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "cross-env NEXT_DISABLE_ESLINT=1 TSC_COMPILE_ON_ERROR=true next build",
    "start": "next start",
    "export": "next export",
    "lint": "next lint",
    "test": "jest --watchAll=false",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --watchAll=false",
    "test:ci": "jest --ci --coverage --watchAll=false --passWithNoTests --verbose"
  },
  "dependencies": {
    "@blocknote/core": "^0.35.0",
    "@blocknote/mantine": "^0.35.0",
